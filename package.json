{
  "name": "dexbreille-gatekeeper-plugin",
  "version": "1.0.0",
  "description": "Gatekeeper through Auth0 M2M Authentication for dexbreille based project.",
  "main": "./dist/plugin.js",
  "types": "./dist/plugin.d.ts",
  "scripts": {
    "compile": "tsc",
    "start": "node ./dist/index.js",
    "dev": "nodemon --exec 'npm run compile && npm run start'",
    "dev-compose": "docker compose -f compose.development.yaml up",
    "install": "npm run compile"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "jose": "^6.1.0",
    "zod": "^4.1.11"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "dist"
    ],
    "ext": "*.ts",
    "delay": 1000
  },
  "dotenvFilenames": [
    ".env",
    ".env.local"
  ],
  "devPort": 8080
}
